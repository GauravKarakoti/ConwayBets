{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "credentialless"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "cross-origin"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/faucet-proxy",
      "destination": "https://faucet.testnet-conway.linera.net"
    },
    {
      "source": "/faucet-proxy/(.*)",
      "destination": "https://faucet.testnet-conway.linera.net/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}